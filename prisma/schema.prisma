generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  user_id    Int    @unique(map: "userId") @default(autoincrement())
  userEmail  String @id @db.VarChar(150)
  userName   String @unique(map: "username") @db.VarChar(50)
  favGame    String @db.VarChar(100)
  teamName   String @db.VarChar(100)
  university String @db.VarChar(200)
  gender     String @db.VarChar(20)
}
